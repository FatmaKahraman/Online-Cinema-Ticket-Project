<%- include ../layouts/header.ejs %>
<div class="page-data">
    <div class="data-table">
        <table>
            <tr>
                <th width="50px">ID</th>
                <th>Movie Name</th>
                <th>Duration</th>
                <th>Points</th>
                <th>Movie Type</th>
                <th>Description</th>
                <th>Action</th>
                <th>Time</th>
            </tr>

            <% if(data.length){

            for(var i = 0;i < data.length;i++) { %>

            <tr>
                <td><%=data[i].movie_id%></td>
                <td><%=data[i].moviename%></td>
                <td><%=data[i].duration%></td>
                <td><%=data[i].points%></td>
                <td><%=data[i].movietype%></td>
                <td><%=data[i].description%></td>
                <td><%=data[i].movies_log_action%></td>
                <td><%=data[i].movies_log_time%></td>
            </tr>
            <% }
            }else{ %>
            <tr>
                <td colspan="6">No Log</td>
            </tr>
            <% } %>
        </table>
        <button onClick="returnAdminPage();">RETURN MAIN PAGE</button>
    </div>
</div>

<%- include ../layouts/footer.ejs %>
